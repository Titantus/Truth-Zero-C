class NeutralizationPhysicsEngine:
    def __init__(self):
        self.hbar = 1.054571817e-34
        self.c = 2.99792458e8
        self.m_c4 = 1.4e-27
        self.m_c3 = 1.6e-25
        self.m_c2 = 1e-60
        self.m_c1 = 1e-80
        self.alpha_c4 = 1.0
        self.alpha_c3 = 1e-6
        self.alpha_c2 = 7.3e-3
        self.alpha_c1 = 1e-38

    def calculate_force(self, r, m, alpha):
        base = alpha * (self.hbar * self.c) / r**2
        yukawa = np.exp(-m * self.c * r / self.hbar)
        return base * yukawa

